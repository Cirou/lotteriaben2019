{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "pausappranzo": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/public/",
            "index": "src/public//index.html",
            "main": "src/public//main.ts",
            "tsConfig": "src/public//tsconfig.app.json",
            "polyfills": "src/public//polyfills.ts",
            "assets": [
              "src/public/assets",
              "src/public/assets/mock",
              "src/public/favicon.ico",
              "src/manifest.json"            ],
            "styles": [
              "node_modules/tether/dist/css/tether.css",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "src/public/assets/css/styles.scss",
              "src/public/assets/css/theme.scss"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.slim.js",
              "node_modules/tether/dist/js/tether.js",
              "node_modules/bootstrap/dist/js/bootstrap.bundle.js",
              {
                "input": "node_modules/document-register-element/build/document-register-element.js"
              }
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/public//environments/environment.ts",
                  "with": "src/public//environments/environment.prod.ts"
                }
              ],
              "serviceWorker": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "pausappranzo:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "pausappranzo:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "pausappranzo:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/public//test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/public//polyfills.ts",
            "tsConfig": "src/public//tsconfig.spec.json",
            "scripts": [
              "node_modules/jquery/dist/jquery.slim.js",
              "node_modules/tether/dist/js/tether.js",
              "node_modules/bootstrap/dist/js/bootstrap.bundle.js"
            ],
            "styles": [
              "node_modules/tether/dist/css/tether.css",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "src/public/assets/css/styles.scss",
              "src/public/assets/css/theme.scss"
            ],
            "assets": [
              "src/public/assets",
              "src/public/favicon.ico",
              "src/manifest.json"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "pausappranzo-e2e": {
      "root": "",
      "sourceRoot": "",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "pausappranzo:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "pausappranzo",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "styleext": "css"
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  }
}
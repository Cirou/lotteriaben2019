{
  "index": "/public/index.html",
  "dataGroups": [
    {
      "name": "dataGroup1",
      "urls": [
        "!/message/"
      ],
      "cacheConfig": {
        "maxSize": 50,
        "maxAge": "3d12h"
      }
    }
  ],
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/public/favicon.ico",
          "/public/index.html",
          "/public/*.css",
          "/public/*.js",
          "/public/*.json"
        ],
        "urls": [
          "/"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/public/assets/**"
        ]
      }
    }
  ]
}